#include <iostream>

using namespace std;

int main() {
    int tinggiSegitiga = 4;
    long long total = 1;

    for (int i = 1; i <= tinggiSegitiga; ++i) {
        long long baris = 1;
        cout << "1";

        for (int j = 1; j <= i; ++j) {
            int bilanganGanjil = (2 * j) + 1;
            baris *= bilanganGanjil;
           cout << " * " << bilanganGanjil;
        }
        total *= baris;
        cout << " = " << baris << endl;
    }
    return 0;
}
